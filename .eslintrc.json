{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier":"error",
        "class-methods-use-this":"off",
        "camelcase":"off",
        "indent":["error",2],
        "prefer-const":"off",
        "no-inner-declarations":"warn",
        "no-plusplus":"off",
        "no-restricted-globals":"off",
        "no-param-reassign":"off",
        "no-underscore-dangle":"off",
        "no-await-in-loop":"off",
        "consistent-return":"off",
    }
}
